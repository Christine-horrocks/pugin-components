<dl {?data.meta} class="meta" {/data.meta}>
    {#data.items}
        {?term}
            {#term}
                <dt>{@t key="{content|s}" data=data /}</dt>
                {#description}
                <dd>{@t key="{content|s}" data=data /}</dd>
                {/description}
            {/term}
        {/term}
    {/data.items}
</dl>